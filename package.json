{
  "name": "react-hapi-fullstack-boilerplate",
  "description": "a fullstack react webapp with redux, hmr, hapi, sequelize",
  "author": "EunseomEom <me@eseom.org> (http://github.com/eseom)",
  "license": "MIT",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/eseom/react-hapi-fullstack-boilerplate"
  },
  "homepage": "https://github.com/eseom/react-hapi-fullstack-boilerplate",
  "keywords": [
    "react",
    "isomorphic",
    "universal",
    "webpack",
    "hapi",
    "sequelize",
    "hot reloading",
    "react-hot-reloader",
    "redux",
    "starter",
    "boilerplate",
    "babel"
  ],
  "babel": {
    "presets": [
      "react",
      "es2015",
      "stage-0"
    ],
    "plugins": [
      "transform-runtime",
      "add-module-exports",
      "transform-decorators-legacy",
      "transform-react-display-name"
    ],
    "env": {
      "development": {
        "plugins": [
          "typecheck",
          [
            "react-transform",
            {
              "transforms": [
                {
                  "transform": "react-transform-catch-errors",
                  "imports": [
                    "react",
                    "redbox-react"
                  ]
                }
              ]
            }
          ]
        ]
      }
    }
  },
  "main": "bin/server.js",
  "scripts": {
    "start": "concurrent --kill-others \"npm run server\"",
    "server": "better-npm-run server",
    "build": "better-npm-run build",
    "postinstall": "node node_modules/phantomjs-prebuilt/install.js && npm run build",
    "lint": "eslint -c .eslintrc src",
    "dev": "concurrent --kill-others \"npm run dev:client\" \"npm run dev:server\"",
    "dev:server": "better-npm-run dev:server",
    "dev:client": "better-npm-run dev:client",
    "test": "karma start",
    "test:node": "better-npm-run test:node",
    "test:node:watch": "better-npm-run test:node:watch",
    "db:upgrade": "sequelize db:migrate",
    "db:downgrade": "sequelize db:migrate:undo",
    "db:create": "sequelize migration:create"
  },
  "betterScripts": {
    "server": {
      "command": "node bin/server",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "production"
      }
    },
    "dev:server": {
      "command": "node bin/server",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "development"
      }
    },
    "dev:client": {
      "command": "node bin/client",
      "env": {
        "NODE_PATH": "./src"
      }
    },
    "build": {
      "command": "webpack --verbose --colors --display-error-details --config webpack/webpack.config.production.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "test:node": {
      "command": "mocha $(find src/server -name '*-test.js') --compilers js:babel-core/register",
      "env": {
        "NODE_ENV": "test"
      }
    },
    "test:node:watch": {
      "command": "mocha $(find src/server -name '*-test.js') --compilers js:babel-core/register --watch",
      "env": {
        "NODE_ENV": "test"
      }
    }
  },
  "dependencies": {
    "babel-loader": "^6.2.1",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-display-name": "^6.3.13",
    "babel-plugin-transform-runtime": "^6.3.13",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "babel-register": "^6.3.13",
    "babel-runtime": "^6.3.19",
    "bcrypt": "^1.0.2",
    "body-parser": "^1.14.1",
    "boom": "^4.2.0",
    "cheerio": "^0.22.0",
    "compression": "^1.6.0",
    "eslint-plugin-flowtype": "^2.30.0",
    "estraverse-fb": "^1.3.1",
    "file-loader": "^0.8.5",
    "hapi": "^16.1.0",
    "hapi-swagger": "^7.6.0",
    "hoist-non-react-statics": "^1.0.3",
    "immutable": "^3.8.1",
    "inert": "^4.1.0",
    "invariant": "^2.2.0",
    "joi": "^10.2.0",
    "less": "^2.5.3",
    "less-loader": "^2.2.1",
    "lru-memoize": "^1.0.0",
    "map-props": "^1.0.0",
    "multireducer": "^2.0.0",
    "node-fetch": "^1.6.3",
    "piping": "^0.3.0",
    "pretty-error": "^1.2.0",
    "react": "0.14.8",
    "react-dom": "0.14.8",
    "react-helmet": "^2.2.0",
    "react-inline-css": "^2.0.0",
    "react-redux": "^4.0.0",
    "react-router": "2.0.0",
    "react-router-bootstrap": "^0.20.1",
    "react-router-redux": "^4.0.0",
    "redux": "^3.0.4",
